{
  "manifest_version": 2,
  "name": "List All Tabs",
  "version": "3.1.3",
  "description": "show a list of all open tabs",
  "short_name": "List All Tabs",
  "author": "Lior Shtivelman",
  "permissions": [
    "tabs",
    "storage"
  ],
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "images/dark_version.png",
    "default_popup": "index.html"
  },
  "icons": {
    "128": "icon_128.png"
  }
}
