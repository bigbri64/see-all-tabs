{
	"manifest_version": 2,
	"name": "List All Tabs",
	"version": "2.1",
	"description": "show a list of all open tabs",
	"short_name": "List All Tabs",
	"author": "Lior Shtivelman",
	"permissions": [
		"tabs"
	],
	"browser_action": {
		"default_icon": "images/arrow-down-on-black-circular-background.png",
		"default_popup": "index.html"
	},
	"icons": {
		"128": "icon_128.png"
	}
}